#: ../libmatewnck/matewnckprop.c:169 ../libmatewnck/matewnckprop.c:171
#: ../libmatewnck/matewnckprop.c:173 ../libmatewnck/matewnckprop.c:262
msgid "NUMBER"
msgstr "संख्या"

